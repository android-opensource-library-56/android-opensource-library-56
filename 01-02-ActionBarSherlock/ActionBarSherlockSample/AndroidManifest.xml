<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.mydns.sys1yagi.android.actionbarsherlocksample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Sherlock.Light.DarkActionBar" >
        <activity
            android:name="jp.mydns.sys1yagi.android.actionbarsherlocksample.ActionBarSherlockSampleActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jp.mydns.sys1yagi.android.actionbarsherlocksample.ListStyleActivity"
            android:label="@string/title_activity_list_style"
            android:theme="@style/Theme.Sherlock.Light" >
        </activity>
        <activity
            android:name="jp.mydns.sys1yagi.android.actionbarsherlocksample.TabStyleActivity"
            android:label="@string/title_activity_tab_style"
            android:theme="@style/Theme.Sherlock.Light.DarkActionBar"
             >
        </activity>
        <activity
            android:name=".DefaultStyleActivity"
            android:label="@string/title_activity_default_action_bar"
            android:theme="@style/Theme.Sherlock.Light.DarkActionBar" >
        </activity>
    </application>

</manifest>